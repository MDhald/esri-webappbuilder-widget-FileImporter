{
  "GPServer": "https://carto-qul.opt.nc/ags/rest/services/converter/GPServer",
  "importTool": "import",
  "async": true,
  "allowed_extensions": [
    "zip",
    "kml",
    "kmz",
    "dwg",
    "dxf",
    "csv",
    "txt",
    "json",
    "geojson",
    "gpx",
    "xls",
    "xlsx",
    "shp"
  ],
  "CAD": {
    "txt_symbol": {
      "color": [
        197,
        38,
        38,
        255
      ],
      "type": "esriTS",
      "verticalAlignment": "bottom",
      "horizontalAlignment": "center",
      "angle": 0,
      "xoffset": 0,
      "yoffset": 0,
      "text": "MyTxt",
      "rotated": false,
      "kerning": true,
      "font": {
        "size": 12.75,
        "style": "normal",
        "variant": "normal",
        "decoration": "none",
        "weight": "normal",
        "family": "Arial"
      }
    },
    "point_symbol": {
      "color": [
        0,
        0,
        128,
        128
      ],
      "size": 6,
      "angle": 0,
      "xoffset": 0,
      "yoffset": 0,
      "type": "esriSMS",
      "style": "esriSMSCircle",
      "outline": {
        "color": [
          0,
          0,
          128,
          255
        ],
        "width": 0.75,
        "type": "esriSLS",
        "style": "esriSLSSolid"
      }
    },
    "line_symbol": {
      "color": [
        207,
        101,
        99,
        255
      ],
      "width": 1.5,
      "type": "esriSLS",
      "style": "esriSLSSolid"
    },
    "polygon_symbol": {
      "color": [
        157,
        216,
        231,
        38
      ],
      "outline": {
        "color": [
          63,
          131,
          150,
          255
        ],
        "width": 1.5,
        "type": "esriSLS",
        "style": "esriSLSSolid"
      },
      "type": "esriSFS",
      "style": "esriSFSSolid"
    }
  },
  "XY":{
    "separators":["\n", "|", ";", ","],
    "X":["point_x", "x", "long", "lon", "longitude"],
    "Y":["point_y", "y", "lat", "latitude"]
  }
}